{
	"name": "slate-template",
	"version": "1.0.0",
	"repository": "git@github.com:az-marketing/slate-template.git",
	"author": "Ryan Dool <ryandool@gmail.com>",
	"license": "MIT",
	"type": "module",
	"dependencies": {
		"@drupal/decoupled-menu-parser": "^1.0.0",
		"@popperjs/core": "^2.9.2",
		"@webcomponents/webcomponentsjs": "^2.5.0",
		"lit": "^3.1.1",
		"regenerator-runtime": "^0.13.7"
	},
	"scripts": {
		"develop": "parcel index.html --open",
		"build": "parcel build src/header.js src/az-footer.js src/external-style.js --no-source-maps"
	},
	"targets": {
		"default": {
			"distDir": "./dist"
		},
		"header": {
			"source": "src/header.js"
		},
		"az-footer": {
			"source": "src/az-footer.js"
		},
		"external-style": {
			"source": "src/external-style.js"
		}
	},
	"devDependencies": {
		"@parcel/watcher": "^2.5.1",
		"parcel": "^2.14.4"
	},
	"browserslist": [
		"last 3 and_chr versions",
		"last 3 chrome versions",
		"last 3 opera versions",
		"last 3 ios_saf versions",
		"last 3 safari versions"
	]
}
